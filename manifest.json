{
  "manifest_version": 2,
  "name": "Skip VOD Ads",
  "version": "0.1",
  "description": "Skip video ads in VOD players",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "icon-128x128.png"
  }
}